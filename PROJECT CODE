Index.aspx.cs

using System;
using System.Collections.Generic;
using System.Linq;
using System.Web;
using System.Web.UI;
using System.Web.UI.WebControls;
using System.Data.SqlClient;
using System.Configuration;
using System.Data;

public partial class index : System.Web.UI.Page
{
    SqlConnection scon;
    string allowed1 = "Yes";
    protected void Page_Load(object sender, EventArgs e)
    {
        string connStr = ConfigurationManager.ConnectionStrings["DBConnectionString"].ConnectionString;
        scon = new SqlConnection(connStr);
        scon.Open();
        SqlCommand sc = new SqlCommand("select top 1 * from Crime where Allow ='" + allowed1 + "' order by NewsId desc", scon);
        SqlDataReader sdr = sc.ExecuteReader();
        sdr.Read();
        DataTable dt = new DataTable();

        DataTable table = new DataTable();
        table.Columns.Add("NewsTitle", typeof(String));
        table.Columns.Add("NewsImage", typeof(string));
        table.Columns.Add("NewsContent", typeof(string));
        table.Columns.Add("NewsReporter", typeof(String));
        table.Columns.Add("NewsDate", typeof(String));
        table.Rows.Add(sdr["NewsTitle"].ToString(), sdr["NewsImage"].ToString(), sdr["NewsContent"].ToString(), sdr["NewsReporter"].ToString(), sdr["NewsDate"].ToString());
       
        scon.Close();
        scon.Open();
        SqlCommand sc1 = new SqlCommand("select top 1 * from Sports where Allow ='" + allowed1 + "' order by NewsId desc", scon);
        SqlDataReader sdr1 = sc1.ExecuteReader();
        sdr1.Read();
        table.Rows.Add(sdr1["NewsTitle"].ToString(), sdr1["NewsImage"].ToString(), sdr1["NewsContent"].ToString(), sdr1["NewsReporter"].ToString(), sdr1["NewsDate"].ToString());
        GridView1.DataSource = table;
        GridView1.DataBind();


    }
}


