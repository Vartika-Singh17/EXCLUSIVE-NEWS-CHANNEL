    <!-- begin post -->
    <div class="single">
    <asp:GridView ID="GridView1" PageSize="4" GridLines="None" 
        runat="server" Style="position:relative; text-align:justify" 
    AutoGenerateColumns="False"  BackColor="White" 
       >
    <Columns>
    <asp:TemplateField>
    <ItemTemplate>
    <table border="0" width="50%"  style="width:auto;">
    <tr><td style="font-size:large; color:red;"  colspan="3"><%#Eval("NewsTitle")%></td></tr>
    <tr><td colspan="3">Created On&nbsp;&nbsp; <%#Eval("NewsDate")%>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; written By&nbsp;&nbsp; <%#Eval("NewsReporter")%></td></tr>
    <tr><td colspan="3">
    <center>
        <asp:Image ID="Image1" Width="400" Height="200" ImageUrl='<%#Eval("NewsImage")%>' runat="server" /><br />
    
    </center>
    <%#Eval("NewsContent")%>
    </td></tr>
    
    </table>
    </ItemTemplate>
    </asp:TemplateField>
    </Columns>
    </asp:GridView>
         
        </div>
