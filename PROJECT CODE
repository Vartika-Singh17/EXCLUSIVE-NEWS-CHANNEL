UserRegistration.aspx.cs

using System;
using System.Collections.Generic;
using System.Linq;
using System.Web;
using System.Web.UI;
using System.Web.UI.WebControls;
using System.Data.SqlClient;
using System.Data;
using System.Configuration;
using System.Net.Mail;
using System.Net.Security;
using System.Net;

public partial class UserRegistration : System.Web.UI.Page
{
    SqlConnection scon;
    protected void Page_Load(object sender, EventArgs e)
    {
        string connStr = ConfigurationManager.ConnectionStrings["DBConnectionString"].ConnectionString;
        scon = new SqlConnection(connStr);
    }
    protected void ImageButton1_Click(object sender, ImageClickEventArgs e)
    {
        String un = TextBox1.Text;
        String pass = TextBox2.Text;
        String name = TextBox3.Text;
        String ut = DropDownList1.SelectedItem.ToString();
        String rt = String.Empty;
        if (ut.Equals("Admin"))
        {
            rt = "NA";
        }
        else
        {
             rt = DropDownList2.SelectedItem.ToString();
        }
        String mob = TextBox6.Text;
        String email = TextBox7.Text;
        String add = TextBox8.Text;
        String sq = TextBox9.Text;
        String ans = TextBox10.Text;
        String doj = TextBox11.Text;
        String dob = TextBox12.Text;
        String gender =String.Empty;
        if (RadioButton1.Checked)
        {
            gender = RadioButton1.Text;
        }
        else
        {
          gender =  RadioButton2.Text;
        }
        scon.Open();
        SqlCommand sc = new SqlCommand("UserRegistrationProc", scon);
        sc.CommandType = CommandType.StoredProcedure;
        sc.Parameters.AddWithValue("@un", un);
        
 sc.Parameters.AddWithValue("@pass",pass);
 sc.Parameters.AddWithValue("@name",name);
 sc.Parameters.AddWithValue("@ut",ut);
 sc.Parameters.AddWithValue("@rt",rt);
 sc.Parameters.AddWithValue("@mn",mob);
 sc.Parameters.AddWithValue("@eid",email);
 sc.Parameters.AddWithValue("@add",add);
 sc.Parameters.AddWithValue("@sq",sq);
 sc.Parameters.AddWithValue("@ans",ans);
 sc.Parameters.AddWithValue("@doj",doj);
 sc.Parameters.AddWithValue("@dob",dob);
 sc.Parameters.AddWithValue("@gen", gender);
 sc.ExecuteNonQuery();
 scon.Close();
 String fromaddress = "prashantkhannalko@gmail.com";
 String frompassword = "kirankhanna";
 String toaddress = email;
 String subject = "User Registration Mail";
 String body = "Congratulation Your Registration Process completed successfully<br>Your username is" + un + " And Password is" + pass;
                var smtp = new System.Net.Mail.SmtpClient();
                    {
                        smtp.Host = "smtp.gmail.com";
                        smtp.Port = 587;
                        smtp.EnableSsl = true;
                        smtp.DeliveryMethod = System.Net.Mail.SmtpDeliveryMethod.Network;
                        smtp.Credentials = new NetworkCredential(fromaddress, frompassword);
                        smtp.Timeout = 20000;
                    }
                    smtp.Send(fromaddress, toaddress, subject, body);
    }
    protected void ImageButton2_Click(object sender, ImageClickEventArgs e)
    {

    }
    protected void DropDownList1_SelectedIndexChanged(object sender, EventArgs e)
    {
        if (DropDownList1.SelectedItem.ToString().Equals("Admin"))
        {
            DropDownList2.Enabled = false;
        }
        else
        {
            DropDownList2.Enabled = true;
        }
    }
}


 Reporter:
This module contains different utilities:
Input: This module contains following details about different reporters and the news posted by them.
Output: The output of this module is stored in the reporter database.
Functionality: This module allows the admin to get the information about the different reporters .

