Login.aspx.cs

using System;
using System.Collections.Generic;
using System.Linq;
using System.Web;
using System.Web.UI;
using System.Web.UI.WebControls;
using System.Data.SqlClient;
using System.Configuration;
using System.Data;

public partial class Login : System.Web.UI.Page
{
    SqlConnection scon;
    protected void Page_Load(object sender, EventArgs e)
    {
        string connStr = ConfigurationManager.ConnectionStrings["DBConnectionString"].ConnectionString;
        scon = new SqlConnection(connStr);
    }
    protected void ImageButton1_Click(object sender, ImageClickEventArgs e)
    {
        scon.Open();
        SqlCommand sc = new SqlCommand("CheckLoginProc", scon);
        sc.CommandType = CommandType.StoredProcedure;
        sc.Parameters.AddWithValue("@un", TextBox1.Text);
        sc.Parameters.AddWithValue("@pass", TextBox2.Text);
        SqlDataReader sdr = sc.ExecuteReader();
        if (sdr.HasRows)
        {
            Session["un"] = TextBox1.Text;
            sdr.Read();
            String ut = sdr["Usertype"].ToString();
            if (ut.Equals("Admin"))
            {
                Response.Redirect("AdminIndex.aspx");
            }



            else
            {
                String rt = sdr["Reportertype"].ToString();
                Session["rt"] = rt;
                Response.Redirect("ReporterIndex.aspx");

            }

        }
        else
        {

        }
    }
    protected void ImageButton2_Click(object sender, ImageClickEventArgs e)
    {
        TextBox1.Text = "";
        TextBox2.Text = "";
        TextBox1.Focus();
    }
}

        
